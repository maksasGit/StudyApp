<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.geometry.Insets?>

<AnchorPane fx:id="root" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.LogInController">
    <padding>
        <Insets top="25" right="25" bottom="25" left="25" />
    </padding>
    <children>
        <Label layoutX="31.0" layoutY="45.0" text="Username" />
        <TextField fx:id="usernameField" layoutX="116.0" layoutY="42.0" prefWidth="150.0" />
        <Label layoutX="31.0" layoutY="97.0" text="Password" />
        <PasswordField fx:id="passwordField" layoutX="116.0" layoutY="94.0" prefWidth="150.0" />
        <Button fx:id="LogInButton" layoutX="116.0" layoutY="152.0" mnemonicParsing="false" onAction="#handleLoginButton" text="Log In" />
    </children>
</AnchorPane>
