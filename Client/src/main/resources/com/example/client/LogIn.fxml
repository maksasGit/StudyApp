<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="root" prefHeight="200.0" prefWidth="300.0" style="-fx-background-color: #49A6B8;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.LogInController">
    <padding>
        <Insets bottom="25" left="25" right="25" top="25" />
    </padding>
    <children>
        <Label layoutX="31.0" layoutY="82.0" text="Логин" textFill="#ea5e3a">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font></Label>
        <TextField fx:id="usernameField" layoutX="116.0" layoutY="78.0" prefWidth="150.0" promptText="Логин" />
        <Label layoutX="27.0" layoutY="141.0" text="Пароль" textFill="#ea5e3a">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font></Label>
        <PasswordField fx:id="passwordField" layoutX="116.0" layoutY="137.0" prefWidth="150.0" promptText="Пароль" />
        <Button fx:id="LogInButton" layoutX="134.0" layoutY="199.0" mnemonicParsing="false" onAction="#handleLoginButton" text="Войти" textFill="#84514e" />
        <Label layoutX="59.0" layoutY="25.0" text="Войдите в свой аккаунт" textFill="#ea5e3a">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </Label>
    </children>
</AnchorPane>